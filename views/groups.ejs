<!DOCTYPE html>
<html lang="en">

<head>
    <%- include( 'layouts/head'); -%>
</head>

<body>
    <div class="container" id="groupPage">
        <%- include( 'layouts/header'); -%>
            <div class="errorField"></div>
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="icon" data-toggle="modal" data-target="#addGroupModal"><i class="mdi-content-add-circle-outline openModalAdd"></i></div>
                    <h1>Groups</h1>
                    <table class="table table-striped table-hover ">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Title</th>
                                <th>Year entered</th>
                                <th>Year ended</th>
                                <th>Semester Now</th>
                                <th>Members student</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                    <span id="listIsClear">List is clear. Please add group.</span>
                </div>
            </div>
            
            <%- include( 'modals/groups/addGroup'); -%>
            <%- include( 'modals/groups/deleteGroup'); -%>
            <%- include( 'modals/groups/editGroup'); -%>
    </div>
    <%- include( 'layouts/footer'); -%>
</body>

</html>
